{
  "name": "wcnode",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "./node_modules/.bin/istanbul test ./node_modules/.bin/_mocha",
    "citest": "JUNIT_REPORT_PATH=xunit.xml JUNIT_REPORT_STACK=1 ./node_modules/.bin/ibrik cover ./node_modules/.bin/_mocha -- --reporter mocha-jenkins-reporter && ./node_modules/.bin/istanbul report cobertura",
    "lint": "./node_modules/.bin/coffeelint *.coffee lib test routes; true",
    "cilint": "./node_modules/.bin/coffeelint --reporter checkstyle *.coffee lib test routes > ./coverage/checkstyle.xml; true"
  },
  "dependencies": {
    "express": "~4.9.0",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "morgan": "~1.3.0",
    "serve-favicon": "~2.1.3",
    "debug": "~2.0.0",
    "stylus": "0.42.3",
    "oriento": "~0.6.0",
    "winston": "~0.8.3",
    "bluebird": "^2.3.11",
    "flake-idgen": "~0.1.4",
    "biguint-format": "~0.2.2",
    "coffee-script": "^1.8.0",
    "lodash": "^2.4.1"
  },
  "devDependencies": {
    "chai": "~1.10.0",
    "mocha": "~2.0.1",
    "supertest": "~0.15.0",
    "istanbul": "~0.3.2",
    "mocha-jenkins-reporter": "^0.1.3",
    "chai-as-promised": "~4.1.1",
    "ibrik": "^2.0.0",
    "coffeelint": "^1.6.1"
  }
}
